<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="xyz.abdalrahman.qumrax2"
    android:versionCode="1"
    android:versionName="first_release">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_qumrax"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_qumrax_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:debuggable="false"

        tools:ignore="HardcodedDebugMode">
        <activity android:name=".MainActivity" android:configChanges="orientation" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-feature android:name="android.hardware.camera2"/>
    

</manifest>